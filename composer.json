{
	"name": "tjm/wiki-site"
	,"description": "Web-site front-end interface for a folder of markdown files, optionally managed with `tjm/wiki`, using Symfony."
	,"authors": [
		{
			"name": "Toby Mackenzie"
			,"email": "public@tobymackenzie.com"
			,"homepage": "https://www.tobymackenzie.com"
		}
	]
	,"license": "0BSD"
	,"autoload": {
		"psr-4": {
			"TJM\\WikiSite\\": "src"
		}
	}
	,"autoload-dev": {
		"psr-4": {
			"TJM\\WikiSite\\Tests\\": "tests"
			,"TJM\\WikiSiteBundle\\": "src"
		}
	}
	,"require": {
		"php": ">=7.4"
		,"league/commonmark": "^2"
		,"symfony/asset": "^5.4"
		,"symfony/framework-bundle": "^5.4"
		,"symfony/mime": "^5.4"
		,"symfony/twig-bundle": "^5.4"
		,"symfony/yaml": "^5.4"
		,"tjm/wiki": "^0.0.1"
	}
	,"require-dev": {
		"symfony/browser-kit": "^5.4"
		,"tjm/dev": "*"
	}
}
